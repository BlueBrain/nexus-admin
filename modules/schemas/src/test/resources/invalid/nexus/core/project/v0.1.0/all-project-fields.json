{
  "@context": {
    "nxv": "https://bluebrain.github.io/nexus/vocabulary/",
    "schema": "http://schema.org/",
    "name": {
      "@id": "schema:name"
    },
    "prefixMappings": {
      "@id": "nxv:prefixMappings"
    },
    "config": {
      "@id": "nxv:config"
    },
    "maxAttachmentSize": {
      "@id": "nxv:maxAttachmentSize"
    },
    "description": {
      "@id": "nxv:description"
    },
    "prefix": {
      "@id": "nxv:prefix"
    },
    "namespace": {
      "@id": "nxv:namespace",
      "@type": "@id"
    }
  },
  "@id": "https://example.nexus.com/api/projects/name",
  "@type":"nxv:Project",
  "name": "{name}",
  "description": "{desc}",
  "prefixMappings": [
    {
      "prefix": "nxv",
      "namespace": "https://bluebrain.github.io/nexus/vocabulary/"
    },
    {
      "prefix": "persons",
      "namespace": "https://shapes-registry.org/commons/person"
    }
  ],
  "config": {
    "maxAttachmentSize": 10
  }
}